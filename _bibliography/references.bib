@article{steinhardt:2004tang,
  title = {The {{Tang Architectural Icon}} and the {{Politics}} of {{Chinese Architectural History}}},
  author = {Steinhardt, Nancy Shatzman},
  year = {2004},
  journal = {The Art Bulletin},
  volume = {86},
  number = {2},
  pages = {228--254},
  issn = {0004-3079},
  doi = {10.2307/3177416},
  abstract = {The modern study of Chinese architecture was initiated in the 1930s by Western-trained Chinese architects, in particular, Liang Sicheng. Even today, his writings dominate the field. This examination of the effects of a single powerful voice on the study of China's architecture of the Tang dynasty (618-907) demonstrates that Chinese architectural history was intertwined with the life and agenda of Liang Sicheng and was reflective of Chinese politics of the tumultuous decades of the 1930s through the 1970s. As a result, Chinese architecture as we know and understand it has become a selective, even exclusionary set of buildings.},
  language = {English}
}
