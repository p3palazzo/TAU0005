@article{haklay:2015new10,
  title = {A New Look at Shelter 131/51 in the Natufian Site of {{Eynan}} (Ain-{{Mallaha}}), {{Israel}}},
  author = {Haklay, Gil and Gopher, Avi},
  year = {2015},
  month = aug,
  journal = {\textsc{PLOS ONE}},
