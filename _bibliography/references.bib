@article{thomas:2021mustatils95,
  title = {The Mustatils: Cult and Monumentality in {{Neolithic}} North-Western {{Arabia}}},
  shorttitle = {The Mustatils},
  author = {Thomas, Hugh and Kennedy, Melissa A. and Dalton, Matthew and McMahon, Jane and Boyer, David and Repper, Rebecca},
  year = {2021},
